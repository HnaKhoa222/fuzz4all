#include <iostream>
inline std::ostream& operator<<(std::ostream& out, bool x) {
    out << x;
    return out;
}
 
int main() {
    bool x, y;
    std::cout << "Hello world" << std::endl;
    std::cout << std::boolalpha << x << std::endl;
    std::cout << std::boolalpha << y << std::endl;
    return 0;
}